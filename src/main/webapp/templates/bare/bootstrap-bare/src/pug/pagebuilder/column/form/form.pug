.col-md-6
    .column-content
        div(data-iwcm-write="!INCLUDE(/components/formsimple/form.jsp, formName=&quot;Bare - example&quot;, )!")
            form#formMailForm-ampquotbare-exampleampquot(action='' method='post' name='formMailForm-ampquotbare-exampleampquot')
                .form-group
                    label.standardLabel(for='meno') Meno *
                    input#meno.required.form-control.inputtext(data-name='Meno' name='meno' type='text' maxlength='20' placeholder='Meno *' title='povinné pole')
                    .help-block.cs-error.cs-error-meno
                .form-group
                    label.standardLabel(for='telefon') Telefón
                    input#telefon.phoneNumber.form-control.inputtext(data-name='Telefon' name='telefon' type='text' maxlength='14' placeholder='Telefon')
                    .help-block.cs-error.cs-error-telefon
                .form-group
                    label.standardLabel(for='sprava') Správa
                    textarea#sprava.form-control.inputtextarea(data-name='Sprava' name='sprava' placeholder='Sprava')
                .form-group.sender
                    .row
                        .col-6.required-info * Označené polia sú povinné
                        .col-6.text-end
                            input.btn.btn-primary(onclick="return invokeWJAjax('formMailForm-ampquotbare-exampleampquot', 'ajaxFormResultContainer', 'bSubmit', '/FormMailAjax.action');" type='submit' value='Odoslat')
